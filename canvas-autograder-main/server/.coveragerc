[run]
source = .
omit =
    tests/*
    venv/*
    */site-packages/*
    __pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

precision = 2
show_missing = True

[html]
directory = htmlcov
